{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node-server"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "PORT": "3001"
  },
  "functions": {
    "api": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
